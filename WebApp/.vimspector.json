{
  "configurations": {
    "launch - netcoredbg": {
      "adapter": "netcoredbg",
      "filetypes": [
        "cs",
        "cshtml"
      ],
      "configuration": {
        "request": "launch",
        "program": "${workspaceRoot}/bin/Debug/net6.0/WebApp.dll",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceRoot}",
        "env": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        }
      }
    }
  }
}
